class test1:
    field gg int[];
    function f() returns string:
        return g();
    end
    function g() returns string:
        self.gg = new int[5];
        self.gg[1] = 1387;
        //print("1");
        return "shakiba khar khob ast";
    end
    public field isInF bool;
    function loop(a:int) returns bool:
        var i = 0;
        while(i < 10) begin
            if(i == a)
                break;
            print(i);
            i++;
        end
        print(i);
        if(i <> 10)
            return false;
        else
            return true;
    end
end

